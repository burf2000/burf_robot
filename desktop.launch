<?xml version="1.0"?>

<launch>
  <node pkg="rviz" type="rviz" name="rviz" args="-d $(find burf_robot)/main.rviz" required="true"/>
<param name="robot_model" textfile="robot.urdf" />
<node pkg="differential_drive" type="diff_tf.py" name="diff_tf">
     <rosparam param="rate">8.0</rosparam>
     <rosparam param="base_width">0.16</rosparam>
  </node>
</launch>
